<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cawabanga.com.tnb_examples" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddAndSubLayoutActivity"
            android:label="AddAndSubLayout"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".GravityLayoutActivity"
            android:label="GravityLayout"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".BgMusicLayoutActivity"
            android:label="BgMusicLayout"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".VisibilityLayoutActivity"
            android:label="VisibilityLayout"
            android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>
